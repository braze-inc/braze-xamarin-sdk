<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TestAppXamarinForms.MenuPage"
             Title="Appboy Forms Sample">
  <ContentPage.Content>
    <StackLayout>
      <Button Text="Log custom event"
              HorizontalOptions="Center"
              VerticalOptions="Center"
              Clicked="OnLogCustomEvent" />
      <Button Text="Show Slideup"
              HorizontalOptions="Center"
              VerticalOptions="Center"
              Clicked="OnShowSlideupIam" />
      <Button Text="Show Modal"
              HorizontalOptions="Center"
              VerticalOptions="Center"
              Clicked="OnShowModalIam" />
      <Button Text="Show Full"
              HorizontalOptions="Center"
              VerticalOptions="Center"
              Clicked="OnShowFullIam" />
    </StackLayout>
  </ContentPage.Content>
</ContentPage>